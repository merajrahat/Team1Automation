<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="Selenium Test Suite" parallel="false">

    <parameter name="browser" value="chrome"/>
    <parameter name="url" value="https://www.weddingwire.com"/>

    <!--if using cloud and mac you can use OSX, for local you should use mac" or "windows" -->

    <parameter name="platform" value="windows"/>

    <parameter name="cloud" value="false"/>
    <parameter name="envName" value="browserstack"/>
    <parameter name="browserVersion" value="86"/>


    <test name="Automation tests">
        <classes>
            <class name ="com.test.AccountSettingsPageTest"/>
            <class name ="com.test.HomePageTest"/>
            <class name ="com.test.ShopNewPageTest"/>
            <class name="com.test.ShoppingCartPageTest"/>
            <class name="com.test.SignInPageTest"/>
            <class name="com.test.SignUpPageTest"/>
            <class name="com.test.UserHomePageTest"/>
            <class name="com.test.DressesPageTest"/>

        </classes>
    </test>
</suite>